<div ng-controller="resultPanelCtrl">
    <div class="row">
        <div class="col-sm-6">
            <label class="control-label">Compression capacity: </label>
        </div>
        <div class="col-sm-6">
            <label class="control-label">{{compressionForce | numberFormatter}} [N]</label>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-6">
            <label class="control-label">Tension capacity: </label>
        </div>
        <div class="col-sm-6">
            <label class="control-label">{{tensionForce | numberFormatter}} [N]</label>
        </div>
    </div>
    <br />
    <table class="table">
        <thead>
            <tr>
                <th class="text-left">#</th>
                <th class="text-left">Load Case</th>
                <th class="text-left">Axial Force [N]</th>
                <th class="text-left">Capacity [Nmm]</th>

            </tr>
        </thead>
        <tbody>
            <tr class="vert-align" ng-repeat="load in loadCaseResults">
                <td class="col-sm-1">
                    <label class="control-label">{{$index + 1}}</label>
                </td>
                <td class="col-sm-2">
                    <label class="control-label">{{load.LoadCase.Name}}</label>
                </td>
                <td class="col-sm-3">
                    <label class="control-label">{{load.LoadCase.NormalForce | numberFormatter}}</label>
                </td>
                <td class="col-sm-3">
                    <label class="control-label">{{load.Mrd | numberFormatter}}</label>
                </td>
            </tr>
        </tbody>
    </table>
</div>
